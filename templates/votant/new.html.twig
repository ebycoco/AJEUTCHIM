{% extends 'base.html.twig' %}

{% block title %}New Votant
{% endblock %}

{% block body %}
	<h1>Create new Votant</h1>
	{% for message in app.flashes('success') %}
		<div class="alert alert-success">
			{{ message }}
		</div>
	{% endfor %}
	{% for message in app.flashes('warning') %}
		<div class="alert alert-warning">
			{{ message }}
		</div>
	{% endfor %}
	{{ include('votant/_form.html.twig') }}

	<a href="{{ path('votant_index') }}">back to list</a>
{% endblock %}
