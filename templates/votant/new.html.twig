{% extends 'baseprofile.html.twig' %}

{% block title %}New Votant
{% endblock %}

{% block body %}
	<div class="form-maj">
		{% for message in app.flashes('success') %}
			<div class="alert alert-success">
				{{ message }}
			</div>
		{% endfor %}
		{% for message in app.flashes('warning') %}
			<div class="alert alert-warning">
				{{ message }}
			</div>
		{% endfor %}
		<div class="container personnelle">
			<h1>Create new Votant</h1>

			{{ include('votant/_form.html.twig') }}
			<a href="{{ path('votant_index') }}">back to list</a>
		</div>
	</div>

{% endblock %}
