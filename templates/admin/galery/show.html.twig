{% extends 'baseAdmin.html.twig' %}

{% block title %}village
{% endblock %}

{% block body %}
	<div
		class="content-page">
		<!-- Start content -->
		<div class="content">
			<div class="container-fluid">
				<h1>Galery</h1>

				<table class="table">
					<tbody>
						<tr>
							<th>Id</th>
							<td>{{ galery.id }}</td>
						</tr>
						<tr>
							<th>Titre</th>
							<td>{{ galery.titre }}</td>
						</tr>
						<tr>
							<th>ImageName</th>
							<td>{{ galery.imageName }}</td>
						</tr>
						<tr>
							<th>CreatedAt</th>
							<td>{{ galery.createdAt ? galery.createdAt|date('Y-m-d H:i:s') : '' }}</td>
						</tr>
						<tr>
							<th>UpdatedAt</th>
							<td>{{ galery.updatedAt ? galery.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
						</tr>
					</tbody>
				</table>

				<a href="{{ path('galery_index') }}">back to list</a>

				<a href="{{ path('galery_edit', {'id': galery.id}) }}">edit</a>

				{{ include('admin/galery/_delete_form.html.twig') }}
				<!-- end row -->
			</div>
			<!-- container-fluid -->
		</div>
		<!-- content -->
		{{ include('includes/_footer_admin.html.twig')}}
	</div>
{% endblock %}
