{% extends 'baseAdmin.html.twig' %}

{% block title %}village
{% endblock %}

{% block body %}
	<div
		class="content-page">
		<!-- Start content -->
		<div class="content">
			<div class="container-fluid">
				<h1>village</h1>

				<table class="table">
					<tbody>
						<tr>
							<th>Id</th>
							<td>{{ village.id }}</td>
						</tr>
						<tr>
							<th>Description</th>
							<td>{{ village.description }}</td>
						</tr>
						<tr>
							<th>Histoire</th>
							<td>{{ village.histoire }}</td>
						</tr>
						<tr>
							<th>ImageName</th>
							<td>{{ village.imageName }}</td>
						</tr>
						<tr>
							<th>CreatedAt</th>
							<td>{{ village.createdAt ? village.createdAt|date('Y-m-d H:i:s') : '' }}</td>
						</tr>
						<tr>
							<th>UpdatedAt</th>
							<td>{{ village.updatedAt ? village.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
						</tr>
					</tbody>
				</table>

				<a href="{{ path('village_index') }}">back to list</a>

				<a href="{{ path('village_edit', {'id': village.id}) }}">edit</a>

				{{ include('admin/village/_delete_form.html.twig') }}
				<!-- end row -->
			</div>
			<!-- container-fluid -->
		</div>
		<!-- content -->
		{{ include('includes/_footer_admin.html.twig')}}
	</div>
{% endblock %}
