{% extends 'baseAdmin.html.twig' %}

{% block title %}Categorie
{% endblock %}

{% block body %}
	<div
		class="content-page">
		<!-- Start content -->
		<div class="content">
			<div class="container-fluid">
				<h1>Categorie</h1>

				<table class="table">
					<tbody>
						<tr>
							<th>Id</th>
							<td>{{ categorie.id }}</td>
						</tr>
						<tr>
							<th>Name</th>
							<td>{{ categorie.name }}</td>
						</tr>
						<tr>
							<th>CreatedAt</th>
							<td>{{ categorie.createdAt ? categorie.createdAt|date('Y-m-d H:i:s') : '' }}</td>
						</tr>
						<tr>
							<th>UpdatedAt</th>
							<td>{{ categorie.updatedAt ? categorie.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
						</tr>
					</tbody>
				</table>

				<a href="{{ path('categorie_index') }}">back to list</a>

				<a href="{{ path('categorie_edit', {'id': categorie.id}) }}">edit</a>

				{{ include('admin/categorie/_delete_form.html.twig') }}
				<!-- end row -->
			</div>
			<!-- container-fluid -->
		</div>
		<!-- content -->
		{{ include('includes/_footer_admin.html.twig')}}
	</div>
{% endblock %}
