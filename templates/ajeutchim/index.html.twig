{% extends 'base.html.twig' %}

{% block title %}Ajeutchim index{% endblock %}

{% block body %}
    <h1>Ajeutchim index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Contenue</th>
                <th>ImageName</th>
                <th>CreatedAt</th>
                <th>UpdatedAt</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for ajeutchim in ajeutchims %}
            <tr>
                <td>{{ ajeutchim.id }}</td>
                <td>{{ ajeutchim.contenue }}</td>
                <td>{{ ajeutchim.imageName }}</td>
                <td>{{ ajeutchim.createdAt ? ajeutchim.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ ajeutchim.updatedAt ? ajeutchim.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('ajeutchim_show', {'id': ajeutchim.id}) }}">show</a>
                    <a href="{{ path('ajeutchim_edit', {'id': ajeutchim.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('ajeutchim_new') }}">Create new</a>
{% endblock %}
