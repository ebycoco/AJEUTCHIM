{% extends 'base.html.twig' %}

{% block title %}President{% endblock %}

{% block body %}
    <h1>President</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ president.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ president.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ president.prenom }}</td>
            </tr>
            <tr>
                <th>Proffession</th>
                <td>{{ president.proffession }}</td>
            </tr>
            <tr>
                <th>DebutedAt</th>
                <td>{{ president.debutedAt ? president.debutedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>FinedAt</th>
                <td>{{ president.finedAt ? president.finedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Contenue</th>
                <td>{{ president.contenue }}</td>
            </tr>
            <tr>
                <th>ImageName</th>
                <td>{{ president.imageName }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ president.createdAt ? president.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ president.updatedAt ? president.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('president_index') }}">back to list</a>

    <a href="{{ path('president_edit', {'id': president.id}) }}">edit</a>

    {{ include('president/_delete_form.html.twig') }}
{% endblock %}
