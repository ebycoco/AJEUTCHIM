<section id="video" class="container-fluid reveal">
	<div class="col-12 text-center">
		<div class="blue-divider"></div>
		<div class="heading">
			<h2 class="mt-3">vidéo</h2>
		</div>
		<p class="lead">
			Les derniers vidéo publier sur la chaine youtube d'AJEUTCHIM.
		</p>
	</div>
	<!-- Three Column Section -->
	<div class="container reveal-1">
		<div class="row my-5">
			{% for video in videos %}
				<div class="col-md-4 mb-3">
					<iframe src="https://www.youtube.com/embed/{{video.lien}}" frameborder="0" allowfullscreen></iframe>
				</div>
			{% endfor %}
		</div>
		<div class="row">
			<div class="col text-center">
				<a href="{{path('video_liste')}}" class="btn btn-primary">
					<i class="fa fa-plus" aria-hidden="true"></i>
				</a>
			</div>
		</div>
	</div>
</section>
